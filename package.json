{
  "name": "@finalytic/integration-mock",
  "private": true,
  "version": "1.0.0",
  "description": "Finalytic Integration mock",
  "repository": "https://github.com/finalytic/integration-mock",
  "license": "UNLICENSED",
  "keywords": [],
  "main": "dist/entry.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "NODE_ENV=development PORT=3000 ts-node-dev ./src/entry.ts",
    "clear": "rm -rf build tsconfig.tsbuildinfo",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "build": "npm run build:tsc && npm run build:esb",
    "build:tsc": "tsc -b tsconfig.json",
    "build:esb": "esbuild ./src/entry.js --bundle --outdir=dist --platform=node --target=node14 --sourcemap --keep-names"
  },
  "dependencies": {
    "@finalytic/client": "2.0.4",
    "@finalytic/integration": "2.0.6",
    "@finalytic/utils": "2.0.3",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "esbuild": "^0.14.25",
    "@jest/globals": "^27.4.4",
    "@types/node": "14.18.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
